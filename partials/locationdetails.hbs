<div class="grid gap-y-5">
    <div class="mt-4">
        <h1 class='text-5xl font-extrabold mb-6 text-ll-blue'> {{neighborhood}} Storage Units </h1>
        <h3 class='text-2xl font-extrabold  text-ll-blue'> Local Locker Storage - Discover Storage Near You </h3>
        <div class="location-reviews-count"></div>        
    </div>
    
    <div class="grid gap-y-5">
        <div class="grid gap-x-2 grid-cols-2">
            <div class='' id='address'>
                <h3>Address</h3>
                <a id="address" class="hover:underline" href="https://www.google.com/maps/dir/?api=1&destination={{name}} {{address.line1}} {{address.city}} {{address.region}} {{address.postalCode}}">{{address.line1}} {{address.city}}, {{address.region}} {{address.postalCode}}<p><i>{{c_crossStreets}}</i></p></a>
            </div>
            <div class=''>
                <h3>Phone</h3>
                <p>{{phoneNumber "US" mainPhone "NATIONAL"}}</p>
            </div>
        </div>

        <div class="grid gap-y-5 sm:grid-cols-2 md:gap-x-5">
            <div class=''>
                <h3>Access Hours</h3>
                {{>hours accessHours}}
            </div>
            <div class=''>
                <h3>Support Hours</h3>
                {{>hours hours}}
            </div>
        </div>
    </div>

</div>
